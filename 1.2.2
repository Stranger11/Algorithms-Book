package dsa;
import java.util.Arrays;
import java.util.Scanner;
import java.util.Random;
/**
 *
 * @author Saugat Poudel
 */
public class DSA {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        Random rand = new Random();
        int n = scanner.nextInt();
        scanner.nextLine();
        
        double[][] intervals = new double[n][2];
        for (int i = 0; i < n; i++) {
            intervals[i][0] = scanner.nextDouble();
            intervals[i][1] = scanner.nextDouble();
            scanner.nextLine();
        }
        
        for (int i = 0; i < n; i++) {
            for (int j = i+1; j < n; j++) {
                if(intersect(intervals[i],intervals[j])){
                    System.out.println("Points "+intervals[i][0]+" "+intervals[i][1]+" and "+intervals[j][0]+ " "+intervals[j][1]+" intersect" );
                }
                
            }
        }
    
    }
    
    public static boolean intersect(double[] a, double[] b){
        if((b[0] >= a[0] && b[0] <= a[1]) || (b[1] >= a[0] && b[1] <= a[1]) || (a[0] >= b[0] && a[0] <= b[1]) || (a[1] >= b[0] && a[1] <= b[1])){
            return true;
        }
        
        return false;
    }
}
